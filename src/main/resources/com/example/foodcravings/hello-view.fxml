<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>

<VBox fx:id="root" spacing="100" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.foodcravings.controller.HelloController" stylesheets="@css/styles.css">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <VBox spacing="5">
        <Button styleClass="shared-button" text="Lägg till" onAction="#onAddFoodButtonClick"/>
        <Button styleClass="shared-button" text="Spara mums" onAction="#onChangeFavoriteClick"/>
        <!--<Button styleClass="shared-button" text="Se all mat" onAction="#onButtonOneClick"/> -->
        <Button styleClass="shared-button" text="Mums" onAction="#onFavoriteFoodClick"/>
        <Button styleClass="shared-button" text="Bläh" onAction="#onDislikedFoodClick"/>
    </VBox>
        <Label fx:id="buttonOne" visible="false"/>
        <ListView fx:id="foodListView" prefHeight="600" prefWidth="300" visible="false"/>
</VBox>
